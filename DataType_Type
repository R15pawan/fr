name = "Rama"
age =15
student = True
weight = 24.5
print("Name:" , name)
print("Data Type of name is ", type(name))
print("age:", age)
print("Data Type of age is ", type(age))
print("student:", student)
print("Data Type of student is ", type(student))
print("weight:", weight)
print("Data Type of weight is ", type(weight))